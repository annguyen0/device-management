{
    "version": 2,
    "builds": [
        {
            "src": "api/devices.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/devices.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}